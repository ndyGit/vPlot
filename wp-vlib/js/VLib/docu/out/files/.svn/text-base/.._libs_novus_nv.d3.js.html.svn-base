                      y(getY(d,i));
            return nv.utils.NaNtoZero(rval);
          })
          .attr(&#x27;height&#x27;, function(d,i) { return nv.utils.NaNtoZero(Math.max(Math.abs(y(getY(d,i)) - y(0)),1)) });

    });

    return chart;
  }

