      //d3.transition(ticks)
          //.attr(&#x27;y&#x27;, function(d,i) {  return y(Math.max(0, getY(d,i))) })
          //.attr(&#x27;height&#x27;, function(d,i) { return Math.abs(y(getY(d,i)) - y(0)) });
