          .style(&#x27;opacity&#x27;, dataBars.length ? 1 : 0)
          .attr(&#x27;transform&#x27;, &#x27;translate(0,&#x27; + x2.range()[0] + &#x27;)&#x27;);
          
      g.select(&#x27;.nv-context .nv-y1.nv-axis&#x27;).transition()
