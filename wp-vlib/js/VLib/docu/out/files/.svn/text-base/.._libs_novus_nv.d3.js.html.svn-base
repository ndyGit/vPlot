          .attr(&#x27;height&#x27;, availableHeight2);

      brushBGenter.append(&#x27;rect&#x27;)
          .attr(&#x27;class&#x27;, &#x27;right&#x27;)
          .attr(&#x27;x&#x27;, 0)
          .attr(&#x27;y&#x27;, 0)
          .attr(&#x27;height&#x27;, availableHeight2);

      var gBrush = g.select(&#x27;.nv-x.nv-brush&#x27;)
          .call(brush);
      gBrush.selectAll(&#x27;rect&#x27;)
