      });

      //============================================================


      //============================================================
      // Functions
      //------------------------------------------------------------

      // Taken from crossfilter (http://square.github.com/crossfilter/)
      function resizePath(d) {
        var e = +(d == &#x27;e&#x27;),
            x = e ? 1 : -1,
            y = availableHeight2 / 3;
        return &#x27;M&#x27; + (.5 * x) + &#x27;,&#x27; + y
            + &#x27;A6,6 0 0 &#x27; + e + &#x27; &#x27; + (6.5 * x) + &#x27;,&#x27; + (y + 6)
            + &#x27;V&#x27; + (2 * y - 6)
            + &#x27;A6,6 0 0 &#x27; + e + &#x27; &#x27; + (.5 * x) + &#x27;,&#x27; + (2 * y)
            + &#x27;Z&#x27;
            + &#x27;M&#x27; + (2.5 * x) + &#x27;,&#x27; + (y + 8)
            + &#x27;V&#x27; + (2 * y - 8)
            + &#x27;M&#x27; + (4.5 * x) + &#x27;,&#x27; + (y + 8)
