    , color = nv.utils.defaultColor()
    , showLegend = true
    , extent
    , brushExtent = null
    , tooltips = true
    , tooltip = function(key, x, y, e, graph) {
        return &#x27;&lt;h3&gt;&#x27; + key + &#x27;&lt;/h3&gt;&#x27; +
               &#x27;&lt;p&gt;&#x27; +  y + &#x27; at &#x27; + x + &#x27;&lt;/p&gt;&#x27;;
