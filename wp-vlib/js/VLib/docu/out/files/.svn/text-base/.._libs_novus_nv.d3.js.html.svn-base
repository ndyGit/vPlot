              d3.select(this).select(&#x27;.left&#x27;)
                .attr(&#x27;width&#x27;,  leftWidth &lt; 0 ? 0 : leftWidth);

              d3.select(this).select(&#x27;.right&#x27;)
                .attr(&#x27;x&#x27;, x2(d[1]))
                .attr(&#x27;width&#x27;, rightWidth &lt; 0 ? 0 : rightWidth);
            });
