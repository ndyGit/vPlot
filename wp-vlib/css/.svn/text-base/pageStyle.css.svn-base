@CHARSET "UTF-8";

html {
    height: 100%;
}

body {
    padding: 10px;
    min-height: 100%;
}

.container {
    width: 100%;
    heigth: 100%;
}

h4 {
    margin-top: 25px;
}
h2{
    text-align:center;
    font-size:20px;
}
.row {
    margin-bottom: 20px;
}

.row {
    margin: 10px;
}

[class*="col-"] {
            padding-left:5px;
            padding-right:5px;
    padding-top: 15px;
    padding-bottom: 15px;
    background-color: #eee;
    border: 1px solid #ddd;
    background-color: #FFFFFF;
    border: 1px dashed #E3E3E3;
}

hr {
    margin-top: 40px;
    margin-bottom: 40px;
}

.plugin {
    text-align: center;
    padding: 5px;
    margin: 2px;
    cursor: pointer;
    list-style: none;
    background-color: #EEEEEE;
    width: 100%;
}

.pluginDrag {
    background-color: white;
    z-index: 1001;
}

.treeRow {
    width: 100%;
    height: 150px;
    background: gray;
    margin: 5px;
    display: inline-block;
    color: white;
    text-align: center;
}

.treeCol {
    background-color: white;
    width: 50px;
    heigth: 100%;
}

.dropRow {
    width: 100%;
    height: 50px;
    border: 1py dashed gray;
}

.element {
    height: 50px;
    line-height: 3em;
    margin: 0 auto;
    padding: 5px;
    text-align: center;
}

.treeNode {
    display: inline-block;
    padding: 2px;
    float: left;
}

.pluginIcon {
    max-width: 25px;
    max-height: 100%;
    color: gray;
}
.jOrgChart {
    margin: 10px;
    padding: 20px;
}

.node p {
    font-family: tahoma;
    font-size: 10px;
    line-height: 11px;
    padding: 2px;
}
.jOrgChart .line {
    height: 20px;
    width: 4px;
}
.jOrgChart .down {
    background-color: #000000;
    margin: 0 auto;
}
.jOrgChart .top {
    border-top: 3px solid #000000;
}
.jOrgChart .left {
    border-right: 4px solid #000000;
}
.jOrgChart .right {
    border-left: 4px solid #000000;
}
.jOrgChart table {
    width:100%;
}
.jOrgChart td {
    padding: 0;
    text-align: center;
    vertical-align: top;
}
.jOrgChart .node {
    border-radius: 8px;
    color: #000000;
    font-size: 10px;
    background-color: #EEEEEE;
    cursor: pointer;
    text-align: center;
    display: inline-block;
    margin: 0 2px;
    padding-left: 3px;
    padding-right: 3px;
    z-index: 10;
}
.drag-active {
    border-style: dotted !important;
}
.drop-hover {
    border-color: #E05E00 !important;
    border-style: solid !important;
}
.form-horizontal {
    position: relative;
    padding: 10px;
}
table,th{
    text-align:center;
}
 .slider-red, .slider-green, .slider-blue {
margin: 15px;
}

.slider-red .ui-slider-range { background: #ef2929; }
.slider-red .ui-slider-handle { border-color: #ef2929; }
.slider-green .ui-slider-range { background: #8ae234; }
.slider-green .ui-slider-handle { border-color: #8ae234; }
.slider-blue .ui-slider-range { background: #729fcf; }
.slider-blue .ui-slider-handle { border-color: #729fcf; }

.plotControls{
    position: absolute;
    right: 5px;
    top: 5px;
}

.vPlot-BaseContainer:-webkit-full-screen  { width: 100%;height:100%; background-color:white}
.vPlot-BaseContainer:-moz-full-screen     { width: 100%;height:100%; background-color:white}
.vPlot-BaseContainer:-ms-full-screen      { width: 100% ;height:100%;background-color:white}
.vPlot-BaseContainer:-o-full-screen       { width: 100%;height:100%; background-color:white}
.vPlot-BaseContainer:full-screen          { width: 100%;height:100%; background-color:white}
#vGraph {
    overflow-x: scroll;
}
