define(['require','core/Utils.vlib'],function(require,Utils) {
    var template = (function() {

        // TREE NODE STRUCTURE
        var Tree = Utils.struct.treeNode;

        var template = new Tree('3d');
        template.config = {
            antialias : true,
            alpha : false,
            scene : {
                width : 'auto',
                height : 'auto'
            },
            camera : {
                x:0,
                y:0,
                z:1000
            }
        }
        var control = new Tree('cameraControl');
        control.config = {
            controlType : 'tracball',
                //controlType : 'orbit',
                pos : {x:0,y:0,z:0}
            };

           var scatter = new Tree('scatterplot');
           scatter.config = {
                scale : {x:400,y:400,z:100},
                material : {
                    particleSize : 20,
                    opacity : 1,
                    image : 'data/disc.png',
                    transparent : true,
                    colors : true
                }
           }
           var data = new Tree('plane');
           data.config = {
                segmentsX : 300,
                segmentsY : 300,
                xMin : -10,
                yMin : -10,
                zMin : -10,
                xMax : 10,
                yMax : 10,
                zMax : 10
           }
           var fun = new Tree('function');
           fun.config = {
                zFun : 'sin(sqrt(x^2+y^2))'
           }


            var axes = new Tree('axes');
            axes.config = {
                valueRange : true,
                heatmapRange : true,
                labels : {
                    'x' : 'x',
                    'y' : 'y',
                    'z' : 'z',
                    'data' : false
                },
                range : {
                    'x' : 'auto',
                    'y' : 'auto',
                    'z' : 'auto'
                },
                numUnits : {
                    'x' : 2,
                    'y' : 2,
                    'z' : 4,
                },
                raster : {
                    xy : {
                        show : false,
                        opacity : 0.2
                    },
                    xz : {
                        show : true,
                        opacity : 0.2
                    },
                    yz : {
                        show : true,
                        opacity : 0.2
                    }
                }
            };
            var heatmap = new Tree('heatmapPoints');
            heatmap.config = {
            numberRange : {
                from : '-1',
                to : '3'
            },
            spectrum : {
                from : '008000',
                to : 'ff0000'
            }
        };

            data.childs.push(heatmap);
            data.childs.push(fun);
            scatter.childs.push(data);
            scatter.childs.push(axes);
            template.childs.push(control);
            template.childs.push(scatter);
            return template;
        })();
        return template;
    })
