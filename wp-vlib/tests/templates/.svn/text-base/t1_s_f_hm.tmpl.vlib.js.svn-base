define(['require','core/Utils.vlib'],function(require,Utils) {
    var template = (function() {

        // TREE NODE STRUCTURE
        var Tree = Utils.struct.treeNode;

        var template = new Tree('3d');
        template.config = {
            antialias : true,
            alpha : false,
            scene : {
                width : 'auto',
                height : 'auto'
            },
            camera : {
                x:0,
                y:0,
                z:1000
            }
        }
        var control = new Tree('cameraControl');
        control.config = {
            controlType : 'tracball',
                //controlType : 'orbit',
                pos : {x:0,y:0,z:0}
            };

           var scatter = new Tree('scatterplot');
           scatter.config = {
                scale : {x:400,y:400,z:400},
                material : {
                    particleSize : 30,
                    opacity : 1,
                    image : 'data/disc.png',
                    transparent : true,
                    colors : true
                }
           }
           var data = new Tree('file');
           data.config = {
                path : 'All_KL_Div.gnuplot.dat'
           }
            var axes = new Tree('axes');
            axes.config = {
                valueRange : true,
                heatmapRange : true,
                labels : {
                    'x' : 'x',
                    'y' : 'y',
                    'z' : 'z',
                    'data' : false
                },
                range : {
                    'x' : 'auto',
                    'y' : 'auto',
                    'z' : 2
                },
                numUnits : {
                    'x' : 2,
                    'y' : 2,
                    'z' : 13,
                },
                raster : {
                    xy : {
                        show : false,
                        opacity : 0.1
                    },
                    xz : {
                        show : false,
                        opacity : 0.1
                    },
                    yz : {
                        show : true,
                        opacity : 0.1
                    }
                }
            };
            var heatmap = new Tree('heatmapPoints');
            heatmap.config = {
            numberRange : {
                from : '-0.5',
                to : '1.5'
            },
            spectrum : {
                from : '008000',
                to : 'ff0000'
            }
        };

            data.childs.push(heatmap);
            scatter.childs.push(data);
            scatter.childs.push(axes);
            template.childs.push(control);
            template.childs.push(scatter);
            return template;
        })();
        return template;
    })
