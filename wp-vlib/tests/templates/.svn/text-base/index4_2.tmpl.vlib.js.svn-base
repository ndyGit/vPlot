define(function(require) {
	var template = (function() {

		// TEST TREE
		var Tree = (function() {
			this.name = '';
			this.config = '';
			this.childs = [];
		});

		var template = new Tree();
		template.name = '3d';
		template.config = {
			scene : {
				width : 'auto',
				height : 'auto'
			},
			camera : {
				x:0,
				y:0,
				z:1000
			}
		}

		var surfaceplot = new Tree();
		surfaceplot.name = 'surfaceplot';
		surfaceplot.config = {
			material : {
				particleSize : 20,
				opacity : 1,
				img : 'data/smiley.png',
				blending : false,
				transparent : true,
				colors : true,
				ambient: 'ffffff'
			},
			scale:{
				x:400,
				y:400,
				z:400
			},
			range : {
				x:'auto',
				y:'auto',
				z:1.5
			}
		};

		var control = new Tree();
		control.name = 'cameraControl';
		control.config = {
				controlType : 'tracball', 
				//controlType : 'orbit', 
				pos : {x:200,y:200,z:0}
		};



		var dataset1 = new Tree();
		dataset1.name = 'dataset';
		dataset1.config = {
			path : 'All_KL_Div.gnuplot.dat',
			data : []
		}

		var dataset2 = new Tree();
		dataset2.name = 'dataset';
		dataset2.config = {
			path : '',
			data : [ {
				x : 0,
				y : -1,
				z : 0
			}, {
				x : 0,
				y : -2,
				z : 0
			}, {
				x : 0,
				y : -3,
				z : 0
			}, {
				x : 0,
				y : -4,
				z : 0
			} ]
		}

		var color1 = new Tree();
		color1.name = 'color';
		color1.config = {
			color : 'd712bd'//d712bd
		};

		var color2 = new Tree();
		color2.name = 'color';
		color2.config = {
			color : '0cf600'
		};

		var heatmapPoints = new Tree();
		heatmapPoints.name = 'heatmapPoints';
		heatmapPoints.config = {
			numberRange : {
				from : '0',
				to : '0.4'
			},
			spectrum : {
				from : 'green',
				to : 'ff0000'
			}
		};
		var axes = new Tree();
		axes.name = 'axes';
		axes.config = {
			labels:{
				x:"x",
				y:"y",
				z:"z ("+surfaceplot.config.range.z+")"
			}	
		};
		var light1 = new Tree();
		light1.name = 'light';
		light1.config = {
				
				type 	: 'point',
				pos		: {
					x	:	0,
					y	:	0,
					z	:	400
				},
				intensity : 110,
				distande : 5,
				color: 'ff0000'
		}
		var light2 = new Tree();
		light2.name = 'light';
		light2.config = {
				
				type 	: 'ambient',
				pos		: {
					x	:	0,
					y	:	0,
					z	:	1000
				},
	
				color: 'ffffff'
		}
		
		dataset1.childs.push(color1);	
		surfaceplot.childs.push(dataset1);
		surfaceplot.childs.push(axes);
		template.childs.push(surfaceplot);
		//template.childs.push(light1); // point
		template.childs.push(light2); // ambient

		template.childs.push(control);
		

		return template;
	})();
	return template;
})